@model List<StudentPortal.Model.Student>
<h1>Students</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
<tr>
    <td>@student.Id</td>
    <td>@student.Name</td>
    <td>@student.Email</td>
    <a>Edit</a>
</tr>
        }
    </tbody>
</table>