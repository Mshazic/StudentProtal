@model List<StudentPortal.Model.Student>
<h1>Students</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
@foreach(var student in Model)
{
     <tr>
    <td>@student.Id</td>
    <td>@student.Name</td>
    <td>@student.Email</td>
    <td>
   <a class="btn btn-primary" asp-asp-controller="Students" asp-action="Edit" asp-asp-route-id="@student.Id">Edit</a>
    </td>
     </tr>
}
    </tbody>
</table>